{
    "name": "brahmjotsingh0/portals",
    "type": "pocketmine-plugin",
    "scripts": {
        "analyze": "vendor/bin/phpstan analyze --level 5 src"
    },
    "require": {
        "phpstan/phpstan": "^1.10.29",
        "pocketmine/pocketmine-mp": "^5",
        "sof3/libasynql": "^4.2.0",
        "sof3/await-generator": "^3.6",
        "paroxity/commando": "^3.2",
        "sof3/pharynx": "^0.3.8"
    },
    "autoload": {
        "psr-0": {
            "BrahmjotSingh0\\Portals\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
